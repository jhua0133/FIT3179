{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "height": "container",
  "background": "#E9F7E9",
  "projection": {"type": "mercator","scale": 520,
    "center": [133.5, -28]},


  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/jhua0133/FIT3179/refs/heads/main/ne_110m_ocean.topojson",
        "format": {"type": "topojson", "feature": "ne_110m_ocean"}
      },
      "mark": {"type": "geoshape", "fill": "#9cc0fa"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/jhua0133/FIT3179/refs/heads/main/ne_110m_graticules_20.topojson",
        "format": {"type": "topojson", "feature": "ne_110m_graticules_10"}
      },
      "mark": {"type": "geoshape", "stroke": "gray", "strokeWidth": 0.5}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/jhua0133/FIT3179/refs/heads/main/gadm41_AUS_1.json",
        "format": {
          "type": "topojson",
          "feature": "georef-australia-state-millesime"
        }
      },
      "mark": {"type": "geoshape", "stroke": "black", "strokeWidth": 1},
      "encoding": {
        "color": {
          "field": "Num_Visitors_per_1000_people",
          "type": "quantitative",
          "scale": {"scheme": "blues"},
          "legend": {"title": "Visitors per 1000 people"}
        },
        "tooltip": [
          {
            "field": "properties.ste_iso3166",
            "type": "nominal",
            "title": "State"
          },
          {
            "field": "Num_Visitors_per_1000_people",
            "type": "quantitative",
            "title": "Visitors per 1,000 people"
          }
        ]
      },
      "transform": [
        {
          "lookup": "properties.ste_iso3166",
          "from": {
            "data": {
              "url": "https://raw.githubusercontent.com/jhua0133/FIT3179/refs/heads/main/normallised_visitors-each-state.csv",
              "format": {"type": "csv"}
            },
            "key": "State",
            "fields": ["Num_Visitors_per_1000_people"]
          }
        }
      ]
    }
  ]
}
